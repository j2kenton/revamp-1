{
  "name": "app-with-login",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "format": "prettier --write .",
    "write-output": "node scripts/write-output.ts",
    "test": "jest && playwright test",
    "test:unit": "jest --testPathPatterns=__tests__",
    "test:unit:watch": "jest --watch",
    "test:unit:coverage": "jest --coverage --testPathPatterns=__tests__",
    "test:unit:chat": "jest --testPathPatterns=__tests__.*chat --coverage",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "test:e2e:chat": "playwright test chat.e2e.ts",
    "test:all": "npm run test:unit && npm run test:e2e",
    "test:ci": "npm run test:unit:coverage && npm run test:e2e",
    "type-check": "tsc --noEmit",
    "validate": "pnpm type-check && pnpm lint",
    "validate-and-test": "pnpm run validate && pnpm run test:all",
    "check": "powershell -NoProfile -Command \"pnpm run validate-and-test 2>&1 | Tee-Object -Variable output; $output | Set-Clipboard; Write-Host '`n[validate:copy] Output copied to clipboard'\"",
    "prepare": "husky"
  },
  "dependencies": {
    "@azure/msal-browser": "^4.26.1",
    "@azure/msal-react": "^3.0.21",
    "@google/genai": "^1.30.0",
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-popover": "^1.1.15",
    "@tanstack/react-query": "^5.90.9",
    "@tanstack/react-virtual": "^3.13.12",
    "@types/ioredis": "^5.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "date-fns": "^4.1.0",
    "framer-motion": "^12.23.24",
    "init": "^0.1.2",
    "ioredis": "^5.8.2",
    "ioredis-mock": "^4.7.0",
    "jose": "^6.1.2",
    "lodash": "^4.17.21",
    "next": "16.0.10",
    "next-auth": "^4.24.13",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-hook-form": "^7.66.0",
    "react-redux": "^9.2.0",
    "redux": "^5.0.1",
    "redux-persist": "^6.0.0",
    "sanitize-html": "^2.17.0",
    "shadcn-ui": "^0.9.5",
    "swr": "^2.3.6",
    "tailwind-merge": "^3.4.0",
    "web-vitals": "^5.1.0",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/jest": "^30.0.0",
    "@types/lodash": "^4.17.20",
    "@types/node": "^20",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/sanitize-html": "^2.16.0",
    "babel-plugin-react-compiler": "^1.0.0",
    "cross-env": "^10.1.0",
    "eslint": "^9",
    "eslint-config-next": "16.0.7",
    "eslint-config-prettier": "^10.1.8",
    "husky": "^9.1.7",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.2.0",
    "msw": "^2.3.0",
    "prettier": "^3.6.2",
    "tailwindcss": "^4",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.2",
    "typescript": "^5",
    "undici": "^7.16.0"
  }
}
